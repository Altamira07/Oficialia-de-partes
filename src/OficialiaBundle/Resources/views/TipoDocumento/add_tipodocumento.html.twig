{% extends '::base.html.twig' %}

{% block titulo %}Agregar tipo de documento{% endblock %}
{% block body %}
<div class="col-md-4 col-md-offset-4">
    <div class="panel panel-default">
        <div class="panel-heading">Agregar tipo documento</div>
        <div class="panel-body">
            {{ form_start(form) }}
            <div class="form-group">
                {{ form_row(form.tipoDocumento,{'attr':{'class':'form-control'}}) }}
            </div>
            {{ form_widget(form.guardar,{'attr':{'class':'btn btn-primary'}}) }}
            {{ form_end(form) }}
        </div>
    </div>
</div>


{% endblock %}