{% extends '::base.html.twig' %}
{% block titulo %}{{ titulo }}{% endblock %}
{% block body %}
<div class="row">
    <div class="col s6 offset-s3">
        <div class="card-panel white">
            <h6>{{ titulo }}</h6>
            <hr>
            {{ form_start(form) }}
                <div class="row">
                    <div class="input-field col s8">
                        {{ form_row(form.institucion,{'attr':{'class':'form-control'}}) }}
                        
                    </div>
                    <div class="col s4">
                    <br>
                        {{ form_widget(form.Guardar,{'attr':{'class':'btn'}}) }}
                        
                    </div>
                </div>
            {{ form_end(form) }}
            
        </div>
    </div>
</div>


{% endblock %}